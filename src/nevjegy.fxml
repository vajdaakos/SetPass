<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="400.0" prefWidth="300.0"
          xmlns="http://javafx.com/javafx/8.0.121" fx:controller="Nevjegy_Controller">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>

    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>

    </rowConstraints>
    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true"
               GridPane.halignment="CENTER" GridPane.valignment="TOP">
        <Image url="@/images/icon.png"/>
    </ImageView>
    <Label fx:id="label" textAlignment="CENTER" GridPane.halignment="CENTER" GridPane.rowIndex="1"
           GridPane.valignment="CENTER"/>
</GridPane>
